<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 
    [템플릿 설명]  
    - 해당 파일은 SQL 문을 작성하는 곳입니다.
-->
<mapper namespace="com.example.demo.Mapper.DvdrentalMapper">

    <select id="selectActorList" resultType="com.example.demo.VO.ActorVO">
        SELECT actor_id,
               first_name,
               last_name,
               last_update
        FROM actor;
    </select>
    
    <insert id="insertData" parameterType="DataVO">
        INSERT INTO data(contents, date)
        values (#{contents}, #{date});
    </insert>

</mapper>